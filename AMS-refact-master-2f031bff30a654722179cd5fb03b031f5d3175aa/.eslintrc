{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "babel",
    "react",
    "promise",
    "jsx-a11y"
  ],
  "env": {
    "browser" : true,
    "mocha": true
  },
  "globals": {
    "__DEV__": false,
    "__TEST__": false,
    "__PROD__": false,
    "__COVERAGE__": false
  },
  "rules": {
    "react/jsx-filename-extension": ["warn", { "extensions": [".js"] }]
  }
}
